<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox id="AnchorPane" prefHeight="500.0" prefWidth="392.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="javafxapplication.Controller.MainController">
    <children>
        <MenuBar>
            <menus>
                <Menu text="Кошкон колдонуучу">
                    <MenuItem mnemonicParsing="false" onAction="#addSeller" text="колдонуучу кошуу" />
                    <MenuItem mnemonicParsing="false" onAction="#listSeller" text="Кошкон колдонуучудын тизмеси" />
                </Menu>
                <Menu text="Квартира жайгашкан район">
                    <MenuItem mnemonicParsing="false" text="Дукон кошуу" onAction="#addКвартира"/>
                    <MenuItem mnemonicParsing="false" onAction="#listShop" text="Квартира жайгашкан райондун тизмеси" />
                    <MenuItem mnemonicParsing="false" onAction="#listShopTreeView" text="Квартира жайгашкан райондун жана Кошкон колдонуучудын тизмеси"></MenuItem>
                </Menu>
                <Menu text="Баа">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Квартира Баа тизмеси" onAction="#listSoldProducts"/>
                        <MenuItem mnemonicParsing="false" text="Квартира кошуу" onAction="#addProduct"/>
                        <MenuItem mnemonicParsing="false" text="Квартира тизмеси" onAction="#listProducts"/>
                    </items>
                </Menu>
                <Menu text="Адрес">
                    <MenuItem mnemonicParsing="false" text="Адрес тизмеси" onAction="#listOrders"/>
                </Menu>
            </menus>
        </MenuBar>
        <VBox fx:id="vbox">
            <VBox.margin>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </VBox.margin>
        </VBox>
    </children>
</VBox>
