<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<FlowPane prefHeight="500.0" prefWidth="453.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="javafxapplication.Controller.House.ListHouseController">
  <children>
    <Button mnemonicParsing="false" onAction="#textTolukta" prefHeight="27.0" prefWidth="88.0" text="Толукта">
      <FlowPane.margin>
        <Insets bottom="15.0" />
      </FlowPane.margin>
    </Button>
    <Button mnemonicParsing="false" onAction="#houseDelete" prefHeight="27.0" prefWidth="88.0" text="Өчүр" textAlignment="LEFT" textFill="RED" textOverrun="LEADING_WORD_ELLIPSIS">
      <FlowPane.margin>
        <Insets bottom="15.0" />
      </FlowPane.margin>
    </Button>
    <TableView fx:id="houseListTableView" prefHeight="200.0" prefWidth="453.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="122.0" text="Кошкон колдонуучу " fx:id="addUser" />
        <TableColumn prefWidth="110.0" text="Квартира жайгашкан район" fx:id="area" />
        <TableColumn prefWidth="110.0" text="Баа" fx:id="price" />
        <TableColumn prefWidth="110.0" text="Дареги" fx:id="adress" />
      </columns>
      <FlowPane.margin>
        <Insets right="15.0" />
      </FlowPane.margin>
    </TableView>
  </children>
</FlowPane>
